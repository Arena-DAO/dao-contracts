{
    "compilerOptions": {
      "target": "es2016",
      "lib": ["ES2021", "DOM"],
      "module": "commonjs",
      "outDir": "build",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": false,
      "skipLibCheck": true
    },
    "exclude": ["codegen"]
  }